FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    p
    {
        solver           GAMG;
        tolerance        1e-7;
        relTol           0.01;
        smoother         DICGaussSeidel;

    }

    pFinal
    {
        $p;
        relTol          0;
    }

    "(U|k|nuTilda)"
    {
        solver          PBiCG;
        preconditioner  DILU;
        tolerance       1e-06;
        relTol          0.001;
    }

    UFinal
    {
        $U;
        relTol          0;
    }
    
    nuTildaFinal
    {
        $nuTilda;
        relTol          0;
    }
    
    kFinal
    {
        $k;
        relTol          0;
    }

}

PIMPLE
{
    nNonOrthogonalCorrectors 0;
    nCorrectors         2;
}



